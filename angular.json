{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "practicing-components": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/practicing-components",
      "sourceRoot": "projects/practicing-components/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/practicing-components",
            "index": "projects/practicing-components/src/index.html",
            "main": "projects/practicing-components/src/main.ts",
            "polyfills": "projects/practicing-components/src/polyfills.ts",
            "tsConfig": "projects/practicing-components/tsconfig.app.json",
            "assets": [
              "projects/practicing-components/src/favicon.ico",
              "projects/practicing-components/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/practicing-components/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/practicing-components/src/environments/environment.ts",
                  "with": "projects/practicing-components/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "practicing-components:build:production"
            },
            "development": {
              "browserTarget": "practicing-components:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "practicing-components:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/practicing-components/src/test.ts",
            "polyfills": "projects/practicing-components/src/polyfills.ts",
            "tsConfig": "projects/practicing-components/tsconfig.spec.json",
            "karmaConfig": "projects/practicing-components/karma.conf.js",
            "assets": [
              "projects/practicing-components/src/favicon.ico",
              "projects/practicing-components/src/assets"
            ],
            "styles": [
              "projects/practicing-components/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    },
    "common-lib": {
      "projectType": "library",
      "root": "projects/common-lib",
      "sourceRoot": "projects/common-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/common-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/common-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/common-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/common-lib/src/test.ts",
            "tsConfig": "projects/common-lib/tsconfig.spec.json",
            "karmaConfig": "projects/common-lib/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "practicing-components"
}
